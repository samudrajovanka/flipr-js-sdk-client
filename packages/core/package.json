{
  "name": "@flipr/core-js-sdk-client",
  "description": "Core Javascript SDK for Flipr feature flags",
  "version": "0.1.1-alpha.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bunup --env inline",
    "dev": "bunup --watch --env inline",
    "typecheck": "tsc --noEmit",
    "release:dev": "standard-version --prerelease alpha --relase-as patch",
    "release:prod": "bun release:prod:patch",
    "release:prod:patch": "standard-version --release-as patch",
    "release:prod:minor": "standard-version --release-as minor",
    "release:prod:major": "standard-version --release-as major"
  },
  "keywords": [
    "feature-flags",
    "flipr",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "bunup": "^0.16.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.13.2"
  }
}
